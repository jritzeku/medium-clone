{
  "name": "medium-clone",
  "version": "1.0.0",
  "description": "medium clone app",
  "main": "server.js",
  "engines": {
    "node": "v16.4.1"
  },
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder/dbSeeder.js",
    "data:destroy": "node backend/seeder/dbSeeder.js -d",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "jigme",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.5",
    "bad-words": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.26.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-joi-validation": "^5.0.0",
    "express-rate-limit": "^6.9.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.0",
    "mongoose-array-validator": "^1.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-cache": "^5.1.2",
    "sharp": "^0.28.3",
    "socket.io": "^4.4.1",
    "store": "^2.0.12",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.9"
  }
}
